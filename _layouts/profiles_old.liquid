---
layout: page
---
<div class="post">
  <article>
    {% if page.profiles %}
      <div class="profiles">
        {% for profile in page.profiles %}
          <div class="profile">
            {% if profile.image %}
              <img src="{{ profile.image }}" alt="Profile Image" class="img-fluid z-depth-1 rounded">
            {% endif %}
            {% if profile.more_info %}
              <div class="more-info">{{ profile.more_info }}</div>
            {% endif %}
          </div>
        {% endfor %}
      </div>
    {% endif %}
  </article>
</div>
